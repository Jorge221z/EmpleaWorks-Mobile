{
  "expo": {
    "name": "EmpleaWorks",
    "slug": "EmpleaWorks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "empleaworks",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jorge221z.EmpleaWorks"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "googleAndroidClientId": "742678869784-uth91mh69d54p816fm4u2ot38e5s0ahq.apps.googleusercontent.com",
      "googleWebClientId": "742678869784-pgutm764ogf80ipdtrdsor885kt8j0bk.apps.googleusercontent.com",
      "eas": {
        "projectId": "35a78599-7800-441a-bcad-553d414d29b2"
      }
    }
  }
}
